    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "react-transform-hmr": "^1.0.2",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "ejs": "^2.4.1",
    "eslint": "^1.10.3",
